{
    "language": "py",
    "description": "python语言grpc服务和sdk的组件库.",
    "author": "hsz",
    "template_type": "components",
    "requires": [
        "pyproxypattern >= 0.0.1",
        "grpcio >= 1.51.1",
        "protobuf >= 3.17.3",
        "grpcio-health-checking >= 1.51.1",
        "grpcio-reflection >= 1.51.1",
        "schema_entry >= 0.1.5",
        "grpcio-tools >= 1.51.1",
        "pyloggerhelper >= 0.0.3"
    ],
    "test_requires": [
        "coverage >= 5.5",
        "mypy >= 0.800",
        "autopep8 >= 1.5.6",
        "pylint >= 2.8.0",
        "pydocstyle>=6.0.0",
        "types-protobuf>=3.19.6"
    ],
    "template_keys": {
        "cross_compiling": {
            "description": "交叉编译",
            "default": "--platform=$TARGETPLATFORM "
        },
        "serv_name": {
            "description": "定义service的名字",
            "default": "{{ project_name }}",
            "ask": true
        },
        "app_name": {
            "description": "定义app的名字",
            "default": "{{ project_name }}"
        },
        "source": {
            "description": "资源名",
            "default": "example"
        }
    },
    "components": {
        "service_schema": {
            "source": "pbschema/_ref_serv_name_fer_.proto.jinja",
            "description": "项目定义service的proto文件",
            "default_path": "pbschema/{{ serv_name }}.proto"
        },
        "aio_serv": {
            "source": "aio/serv",
            "description": "grpc异步服务端模块模板",
            "default_path": "{{ project_name }}/serv"
        },
        "aio_nogen_serv": {
            "source": "aionogen/serv",
            "description": "grpc异步服务端模块模板",
            "default_path": "{{ project_name }}/serv"
        },
        "sync_serv": {
            "source": "sync/serv",
            "description": "grpc同步服务端模块模板",
            "default_path": "{{ project_name }}/serv"
        },
        "sync_nogen_serv": {
            "source": "nogen/serv",
            "description": "grpc同步服务端模块模板",
            "default_path": "{{ project_name }}/serv"
        },
        "aio_sdk": {
            "source": "aio/sdk",
            "description": "grpc异步sdk模块模板",
            "default_path": "{{ project_name }}/sdk"
        },
        "aio_nogen_sdk": {
            "source": "aionogen/sdk",
            "description": "grpc异步sdk模块模板",
            "default_path": "{{ project_name }}/sdk"
        },
        "sync_sdk": {
            "source": "sync/sdk",
            "description": "grpc同步sdk模块模板",
            "default_path": "{{ project_name }}/sdk"
        },
        "sync_nogen_sdk": {
            "source": "nogen/sdk",
            "description": "grpc同步sdk模块模板",
            "default_path": "{{ project_name }}/sdk"
        },
        "aio_serv_interceptor": {
            "source": "aio_interceptor/serv_interceptor",
            "description": "grpc异步服务端模块拦截器模板",
            "default_path": "{{ project_name }}/serv/interceptor"
        },
        "aio_serv_interceptor_source": {
            "source": "aio_interceptor/serv_interceptor/_ref_source_fer_.py.jinja",
            "description": "grpc异步服务端模块拦截器文件模板",
            "default_path": "{{ project_name }}/serv/interceptor/{{ source }}.py"
        },
        "sync_serv_interceptor": {
            "source": "sync_interceptor/serv_interceptor",
            "description": "grpc同步服务端模块拦截器模板",
            "default_path": "{{ project_name }}/serv/interceptor"
        },
        "sync_serv_interceptor_source": {
            "source": "sync_interceptor/serv_interceptor/_ref_source_fer_.py.jinja",
            "description": "grpc异步服务端模块拦截器文件模版",
            "default_path": "{{ project_name }}/serv/interceptor/{{ source }}.py"
        },
        "aio_sdk_interceptor": {
            "source": "aio_interceptor/sdk_interceptor",
            "description": "grpc异步sdk模块拦截器模板",
            "default_path": "{{ project_name }}/sdk/interceptor"
        },
        "aio_sdk_interceptor_source": {
            "source": "aio_interceptor/sdk_interceptor/_ref_source_fer_.py.jinja",
            "description": "grpc异步sdk端模块拦截器文件模板",
            "default_path": "{{ project_name }}/sdk/interceptor/{{ source }}.py"
        },
        "sync_sdk_interceptor": {
            "source": "sync_interceptor/sdk_interceptor",
            "description": "grpc同步sdk模块拦截器模板",
            "default_path": "{{ project_name }}/sdk/interceptor"
        },
        "sync_sdk_interceptor_source": {
            "source": "sync_interceptor/sdk_interceptor/_ref_source_fer_.py.jinja",
            "description": "grpc同步sdk模块拦截器模板",
            "default_path": "{{ project_name }}/sdk/interceptor/{{ source }}.py"
        },
        "servinit": {
            "source": "__init__.py.jinja",
            "description": "serv作为模块时的模块入口文件"
        },
        "main": {
            "source": "__main__.py.jinja",
            "description": "定义复杂项目的入口函数",
            "default_path": "__main__.py"
        },
        "dockerfile": {
            "source": "dockerfile.jinja",
            "description": "构造项目镜像使用的dockerfile",
            "default_path": "dockerfile"
        }
    }
}